#!/bin/bash
cd ~/work/device/xiaomi/fleur/prebuilts
aria2c https://github.com/RT1648/Extras/releases/download/13.0.9.0/vendor.img
ls
cd ~/work
git clone https://github.com/Evolution-X/packages_apps_GCamGOPrebuilt.git packages/apps/GCamGOPrebuilt
rm -rf platfrom/frameworks/av
rm -rf platform/packages/modules/Bluetooth
git clone https://github.com/RT1648/platform_packages_modules_Bluetooth.git platform/packages/modules/Bluetooth
git clone https://github.com/RT1648/platform_frameworks_av.git platform/frameworks/av
rm -rf ~/work/platform/system/sepolicy/vendor/hal_light_default.te
rm -rf ~/work/platform/system/sepolicy/vendor/rild.te
rm -rf ~/work/system/sepolicy/vendor/hal_light_default.te
rm -rf ~/work/system/sepolicy/vendor/rild.te
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 80G
ccache -o compression=true
ccache -z
. build/envsetup.sh
blissify -g fleur & sleep 95m
