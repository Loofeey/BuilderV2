#!/bin/bash
cd ~/work/device/xiaomi/fleur/prebuilts
aria2c https://github.com/ShazuxD/extras/releases/download/v1.0.2/vendor.img
ls
cd ~/work
cd packages/modules/Bluetooth &&git fetch https://github.com/ShazuxD/packages_modules_Bluetooth &&git cherry-pick 61fe376 &&cd ../../.. &&cd frameworks/av &&git fetch https://github.com/ShazuxD/frameworks_av &&git cherry-pick 75cb93e &&git cherry-pick cf0f642 &&cd ../..
. build/envsetup.sh
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 60G
ccache -o compression=true
ccache -z
lunch "$LUNCH_COMBO"
make $TARGET -j8
