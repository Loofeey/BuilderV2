#!/bin/bash
cd ~/work/device/xiaomi/fleur/prebuilts
aria2c https://github.com/RT1648/Extras/releases/download/13.0.9.0.SEKMIXM/vendor.img
cd ~/work
. build/envsetup.sh
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 100G
ccache -o compression=true
ccache -z
lunch "$LUNCH_COMBO"
mka $TARGET -j8
