#!/bin/bash
cd ~/work/device/xiaomi/fleur/prebuilts
aria2c https://github.com/RT1648/Extras/releases/download/13.0.8.0.SKEMIXM/vendor.img
ls
cd ~/work
rm -rf packages/modules/Bluetooth
git clone https://github.com/DerpFest-AOSP/packages_modules_Bluetooth.git packages/modules/Bluetooth
rm -rf frameworks/av
git clone https://github.com/DerpFest-AOSP/frameworks_av.git frameworks/av
. build/envsetup.sh
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 90G
ccache -o compression=true
ccache -z
lunch "$LUNCH_COMBO"
mka $TARGET -j$(nproc --all) & sleep 95m
